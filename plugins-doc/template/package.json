{
  "name": "naimo-plugin-template",
  "version": "1.0.0",
  "description": "Naimo Tools 插件开发模板",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run type-check && npm run build:main && npm run build:preload",
    "build:main": "vite build",
    "build:preload": "node scripts/build-preload.js",
    "preview": "vite preview",
    "add-electron-types": "$env:ELECTRON_SKIP_DOWNLOAD=1; pnpm install electron --save-dev",
    "type-check": "tsc --noEmit",
    "deploy": "node scripts/deploy-to-build.js"
  },
  "keywords": [
    "naimo",
    "plugin",
    "electron"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.5"
  },
  "dependencies": {}
}