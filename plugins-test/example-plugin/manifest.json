{
  "id": "example-plugin",
  "icon": "🎯",
  "name": "示例插件",
  "version": "1.0.0",
  "author": "Naimo Tools",
  "description": "新架构示例插件 - 展示懒加载架构的完整用法",
  "category": "developer_essentials",
  "enabled": true,
  "main": "./index.html",
  "preload": "./preload.js",
  "feature": [
    {
      "path": "hello-world",
      "name": "Hello World",
      "icon": "👋",
      "description": "最简单的示例功能",
      "weight": 100,
      "type": "text",
      "lifecycleType": "reuse",
      "singleton": true
    },
    {
      "path": "img-processor",
      "name": "图片处理器",
      "icon": "📝",
      "description": "处理搜索图片",
      "weight": 90,
      "type": "img",
      "lifecycleType": "new",
      "singleton": false
    },
    {
      "path": "file-counter",
      "name": "文件计数器",
      "icon": "📊",
      "description": "统计附件文件数量",
      "weight": 80,
      "type": "text",
      "showInModes": [
        "attachment"
      ],
      "lifecycleType": "reuse",
      "singleton": true
    }
  ],
  "settings": [
    {
      "name": "apiKey",
      "title": "API 密钥",
      "description": "请输入你的 API 密钥",
      "type": "input",
      "defaultValue": "",
      "required": false
    },
    {
      "name": "enableNotification",
      "title": "启用通知",
      "description": "是否启用桌面通知",
      "type": "switch",
      "defaultValue": true,
      "required": false
    },
    {
      "name": "language",
      "title": "语言",
      "description": "选择界面语言",
      "type": "select",
      "defaultValue": "zh",
      "option": {
        "options": [
          {
            "label": "中文",
            "value": "zh"
          },
          {
            "label": "English",
            "value": "en"
          }
        ]
      }
    }
  ]
}